<!DOCTYPE html>
<html>
<head>
  <title>Seatyourself</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>



  <header>
    <nav>
      <navbar>
        <a href=" /restaurants">Restaurant Listings</a>
        <a href="/sessions/new">Log In</a>
        <a href="/users/new">Sign Up</a>
        <%= link_to "Log out", session_path("current"), :method => :delete %>
      </navbar>
    </nav>
  </header>

  <main>
    <% if  flash[:notice] %>
      <%= flash[:notice] %>
    <% end %>

    <%= yield %>


  </main>

</body>
</html>
